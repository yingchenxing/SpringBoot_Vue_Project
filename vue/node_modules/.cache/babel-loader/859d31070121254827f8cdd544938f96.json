: 10px 0\">\r\n      <el-upload\r\n          action=\"http://localhost:9090/file/upload\" style=\"display: inline-block\" :show-file-list=\"false\"\r\n          :on-success=\"handleFileUploadSuccess\"\r\n      >\r\n        <el-button type=\"primary\" class=\"ml-5\">Upload<i class=\"el-icon-top\"></i></el-button>\r\n      </el-upload>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='Yes'\r\n          cancel-button-text='No'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"Are you sure to remove the users selected?\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">Delete<i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n\r\n\r\n    </div>\r\n    <el-table :data=\"tableData\" border stripe header-cell-class-name=\"'headerBg'\"\r\n              @selection-change=\"handleSelectChange\">\r\n      <el-table-column\r\n          type=\"selection\"\r\n          width=\"55\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" >\r\n      </el-table-column>\r\n      <el-table-column prop=\"name\" label=\"Name\" >\r\n      </el-table-column>\r\n      <el-table-column prop=\"type\" label=\"Type\">\r\n      </el-table-column>\r\n      <el-table-column prop=\"size\" label=\"Size\">\r\n      </el-table-column>\r\n      <el-table-column label=\"Enable\">\r\n        <template slot-scope=\"scope\">\r\n          <el-switch v-model=\"scope.row.enable\" active-color=\"#13ce66\" inactive-color=\"#ccc\"></el-switch>\r\n        </template>\r\n      </el-table-column>\r\n<!--      <el-table-column prop=\"download\" label=\"Download\">-->\r\n\r\n<!--      </el-table-column>-->\r\n      <el-table-column label=\"Operate\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"download(scope.row.url)\">Download</el-button>\r\n          <el-popconfirm\r\n              class